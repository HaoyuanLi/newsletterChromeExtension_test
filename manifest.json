{
  "manifest_version": 2,

  "name": "SPPH Weekly Newsletter Generator",
  "description": "This extension generates the newsletter",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://services.calendar.events.ubc.ca/",
    "http://*.calendar.events.ubc.ca/",
    "http://*.events.ubc.ca/"
  ]
}
